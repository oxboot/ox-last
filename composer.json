{
  "name": "oxboot/ox",
  "description": "Ox - Command-line server control panel & deployment tool written in PHP",
  "keywords": ["framework", "laravel", "console", "ox", "cli"],
  "homepage": "https://oxboot.com/ox",
  "type": "project",
  "license": "MIT",
  "support": {
    "source": "https://github.com/oxboot/ox",
    "issues": "https://github.com/oxboot/ox/issues"
  },
  "authors": [
    {
      "name": "Zorca",
      "email": "vs@zorca.org"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel-zero/foundation": "5.6.*",
    "illuminate/config": "5.6.*",
    "illuminate/console": "5.6.*",
    "illuminate/container": "5.6.*",
    "illuminate/filesystem": "5.6.*",
    "illuminate/cache": "5.6.*",
    "illuminate/events": "5.6.*",
    "symfony/debug": "4.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "symfony/thanks": "^1.0",
    "illuminate/log": "5.6.*"
  },
  "autoload": {
    "psr-4": {
      "Ox\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": ["ox"]
}
